# Array 1 Dimensi
angka = list(range(1, 6))   # [1, 2, 3, 4, 5]
print("Array 1D:", angka)

# Array 2 Dimenai
arr2d = [
    [1, 2, 3],
    [4, 5, 6],
    [7, 8, 9]
]
print(arr2d[1][2])  # baris ke-2, kolom ke-3 â†’ 6

# Array 3 Dimensi (3 lapisan)
kubus = [[[x + y + z for z in range(2)] for y in range(2)] for x in range(2)]
print("Array 3D:", kubus)
